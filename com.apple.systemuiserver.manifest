	&lt;key&gt;disk-image&lt;/key&gt;
	&lt;array/&gt;
	&lt;key&gt;dvd&lt;/key&gt;
	&lt;array/&gt;
	&lt;key&gt;harddisk-external&lt;/key&gt;
	&lt;array&gt;
		&lt;string&gt;eject&lt;/string&gt;
		&lt;string&gt;alert&lt;/string&gt;
	&lt;/array&gt;
	&lt;key&gt;harddisk-internal&lt;/key&gt;
	&lt;array/&gt;
&lt;/dict&gt;</string>
